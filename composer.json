{
    "name": "itvoland/laravel-achievements",
    "description": "Achievements for Laravel 6+",
    "license": "MIT",
    "authors": [
        {
            "name": "Volodymyr Davydenko",
            "email": "vovadavydenko35@gmail.com"
        }
    ],
    "autoload": {
        "classmap": [
            "src/Migrations"
        ],
        "psr-4": {
            "ITVOLAND\\Achievements\\": "src/",
            "ITVOLAND\\Tests\\": "tests/"
        }
